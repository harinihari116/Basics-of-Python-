#logical_operators
def logical_operations():
    a = int(input("Enter a number A: "))
    b = int(input("Enter a number B: "))

    print("Logical AND:", bool(a) and bool(b))
    print("Logical OR:", bool(a) or bool(b))
    print("Logical NOT A:", not bool(a))
    print("Logical NOT B:", not bool(b))

# Call the function
logical_operations()
